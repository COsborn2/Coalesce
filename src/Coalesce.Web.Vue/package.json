{
  "name": "coalesce-web-vue",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build-lib": "yarn --cwd ../coalesce-vue && yarn --cwd ../coalesce-vue build-local && yarn add ../coalesce-vue/local-latest.tgz",
    "build": "yarn run build-lib && vue-cli-service build",
    "test": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "coalesce-vue": "../coalesce-vue/local-latest.tgz",
    "vue": "2.6.10",
    "vue-class-component": "7.0.2",
    "vue-property-decorator": "8.1.0",
    "vue-router": "3.0.6",
    "vuetify": "1.5.13"
  },
  "devDependencies": {
    "@types/jest": "^23.1.3",
    "@vue/cli-plugin-babel": "3.6.0",
    "@vue/cli-plugin-typescript": "3.6.0",
    "@vue/cli-plugin-unit-jest": "3.6.3",
    "@vue/cli-service": "3.6.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "aspnet-webpack": "^3.0.0",
    "babel-core": "^7.0.0-0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.0.3",
    "ts-jest": "^22.4.6",
    "typescript": "3.4.5",
    "vue-template-compiler": "2.6.10",
    "webpack-hot-middleware": "^2.22.3"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "<rootDir>/(tests/unit/**/*.spec.(ts|tsx|js)|**/__tests__/*.(ts|tsx|js))"
    ]
  }
}
